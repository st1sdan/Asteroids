config_version=5

[application]

config/name="Asteroids"
config/tags=PackedStringArray("kursovaya")
run/main_scene="res://Scenes/menu.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
boot_splash/bg_color=Color(0, 0, 0.141176, 1)
boot_splash/image="res://Assets/LogoFullscreen.png"
config/icon="res://Assets/icon.svg"

[audio]

buses/default_bus_layout="res://Config/default_bus_layout.tres"

[autoload]

Globals="*res://Scripts/globals.gd"
SilentWolf="*res://addons/silent_wolf/SilentWolf.gd"

[collada]

use_ambient=true

[debug]

gdscript/warnings/unused_variable=0

[display]

window/size/viewport_width=480
window/size/viewport_height=320
window/size/initial_screen=1
window/size/always_on_top=true
window/stretch/mode="canvas_items"
window/vsync/vsync_mode=2
mouse_cursor/tooltip_position_offset=Vector2(16, 16)

[editor_plugins]

enabled=PackedStringArray()

[gui]

theme/custom_font="res://Config/PressStart2P-Regular.ttf"

